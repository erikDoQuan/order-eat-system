{
  "name": "@loyalty-system/admin-portal",
  "version": "1.0.0",
  "private": true,
  "description": "Vite - Admin Portal Template",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "app:down": "docker-compose stop app",
    "app:log": "docker-compose logs -f app",
    "app:up": "docker-compose up -d app",
    "build": "vite build",
    "build:production": "cross-env NODE_ENV=production vite build --mode production",
    "build:stage": "cross-env NODE_ENV=production vite build --mode stage",
    "clean": "rimraf .next",
    "commit": "better-commits",
    "dev": "concurrently \"pnpm dev:shared-universal\" \"pnpm dev:shared-web\" \"pnpm dev:ui\" \"pnpm dev:app\"",
    "dev:app": "vite",
    "dev:shared-universal": "pnpm --filter @loyalty-system/shared-universal dev",
    "dev:shared-web": "pnpm --filter @loyalty-system/shared-web dev",
    "dev:ui": "pnpm --filter @loyalty-system/react-web-ui-shadcn dev",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "knip": "knip",
    "lint": "eslint",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "cross-env VITE_CJS_IGNORE_WARNING=true vitest --watch=false",
    "test:cov": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2eui": "playwright test --ui",
    "test:ui": "vitest --ui",
    "test:uicov": "vitest --ui --coverage.enabled",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "viewtsconfig": "tsc -p ./tsconfig.json --showConfig"
  },
  "prettier": "@loyalty-system/prettier-config",
  "dependencies": {
    "@artsy/fresnel": "^7.1.4",
    "@blocknote/core": "^0.24.1",
    "@blocknote/react": "^0.24.1",
